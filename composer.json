{
    "name": "piedweb/github",
    "type": "library",
    "description": "Github Meta Data Repo Extractor without api",
    "keywords": [
        "Pied Web",
        "Github"
    ],
    "homepage": "https://dev.piedweb.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin D. (ak Pied Web)",
            "email": "contact@robin-d.fr",
            "homepage": "https://piedweb.com"
        }
    ],
    "require": {
        "php" : "~7.1",
        "ropendev/simple_html_dom": "^1.7",
        "ropendev/curl": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "PiedWeb\\Github\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PiedWeb\\Github\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    }
}
